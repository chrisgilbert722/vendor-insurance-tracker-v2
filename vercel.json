{
  "version": 2,
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "pages/**/*.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/pages/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
